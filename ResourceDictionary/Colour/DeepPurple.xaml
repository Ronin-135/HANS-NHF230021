<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:drawing2d="clr-namespace:System.Drawing.Drawing2D;assembly=System.Drawing"
    xmlns:machine="clr-namespace:Machine"
    xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
    xmlns:sys="clr-namespace:System;assembly=mscorlib"
    xmlns:views="clr-namespace:WPFMachine.Views">
    <SolidColorBrush x:Key="BatTypeOKTypeBrush" Color="Green" />
    <SolidColorBrush x:Key="BatTypeNGTypeBrush" Color="Red" />
    <!--<SolidColorBrush x:Key="BatTypeFake" Color="Blue" />-->
    <SolidColorBrush x:Key="BatTypeBKFillTypeBrush" Color="LightGreen" />
    <SolidColorBrush x:Key="BatTypeFakeBorderBrush" Color="Blue" />

    <!--  自定义ContextMenu的外观样式  -->
    <Style x:Key="ContextMenuStyle" TargetType="{x:Type ContextMenu}">
        <Setter Property="SnapsToDevicePixels" Value="True" />
        <Setter Property="OverridesDefaultStyle" Value="True" />
        <Setter Property="Grid.IsSharedSizeScope" Value="True" />
        <Setter Property="Focusable" Value="False" />
        <Setter Property="HasDropShadow" Value="True" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type ContextMenu}">
                    <Border
                        Name="Border"
                        Background="White"
                        BorderBrush="#888888"
                        BorderThickness="1">
                        <StackPanel IsItemsHost="True" KeyboardNavigation.DirectionalNavigation="Cycle" />
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger Property="HasDropShadow" Value="true">
                            <Setter TargetName="Border" Property="Padding" Value="0,3,0,3" />
                            <Setter TargetName="Border" Property="CornerRadius" Value="4" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>


    <sys:Int32 x:Key="PltTypeDefaultBorderThickness">1</sys:Int32>
    <SolidColorBrush x:Key="PltTypeOKBatteryFakePltTypeBorderBrush" Color="Blue" />
    <sys:Int32 x:Key="PltTypeOKBatteryFakeBorderThickness">2</sys:Int32>
    <Style x:Key="paramaterStyle" TargetType="TextBox">
        <Setter Property="Margin" Value="5" />
        <Setter Property="Height" Value="30" />

    </Style>
    <views:BoolToConverter x:Key="Negation">
        <views:BoolToConverter.TrueValue>
            <sys:Boolean>False</sys:Boolean>
        </views:BoolToConverter.TrueValue>
        <views:BoolToConverter.FalseValue>
            <sys:Boolean>True</sys:Boolean>
        </views:BoolToConverter.FalseValue>
    </views:BoolToConverter>



    <VisualBrush
        x:Key="DotFillBrush"
        TileMode="Tile"
        Viewbox="0,0,12,12"
        ViewboxUnits="Absolute"
        Viewport="0,0,10,10"
        ViewportUnits="Absolute">
        <VisualBrush.Visual>
            <Ellipse
                Width="10"
                Height="10"
                Fill="#00C0FF" />
        </VisualBrush.Visual>
    </VisualBrush>
    <VisualBrush
        x:Key="BatTypeFakeTypeBrush"
        TileMode="Tile"
        Viewbox="0,0,10,10"
        ViewboxUnits="Absolute"
        Viewport="0,0,10,10"
        ViewportUnits="Absolute">
        <VisualBrush.Visual>
            <Canvas>
                <Rectangle
                    Width="10"
                    Height="10"
                    Fill="YellowGreen" />
                <Path Data="M 0 0 l 10 10" Stroke="Blue" />
                <Path Data="M 0 10 l 10 -10" Stroke="Blue" />
            </Canvas>
        </VisualBrush.Visual>
    </VisualBrush>


    <!--  触发器  -->
    <Style x:Key="PalletStyle" TargetType="Border">
        <Style.Triggers>
            <DataTrigger Binding="{Binding Type}" Value="Invalid">
                <Setter Property="BorderBrush" Value="{DynamicResource PrimaryHueLightBrush}" />
                <Setter Property="Background" Value="Transparent" />
                <Setter Property="Visibility" Value="Hidden" />
            </DataTrigger>
            <MultiDataTrigger>
                <MultiDataTrigger.Conditions>
                    <Condition Binding="{Binding Type}" Value="OK" />
                    <Condition Binding="{Binding WhetherThereAreFakeBatteries}" Value="True" />
                </MultiDataTrigger.Conditions>
                <Setter Property="BorderBrush" Value="Blue" />
            </MultiDataTrigger>
            <!--<MultiDataTrigger>
                <MultiDataTrigger.Conditions>
                    <Condition Binding="{Binding Type}" Value="OK" />
                    <Condition Binding="{Binding WhetherThereAreFakeBatteries}" Value="False" />
                    <Condition Binding="{Binding IsThereABattery}" Value="False" />
                </MultiDataTrigger.Conditions>
                <Setter Property="BorderBrush" Value="{DynamicResource PrimaryHueLightBrush}" />
                <Setter Property="Background" Value="DarkGray" />
            </MultiDataTrigger>-->
            <MultiDataTrigger>
                <MultiDataTrigger.Conditions>
                    <Condition Binding="{Binding Type}" Value="OK" />
                    <Condition Binding="{Binding WhetherThereAreFakeBatteries}" Value="False" />
                    <Condition Binding="{Binding IsThereABattery}" Value="True" />
                </MultiDataTrigger.Conditions>
                <Setter Property="BorderBrush" Value="{DynamicResource PrimaryHueLightBrush}" />
                <Setter Property="Background" Value="Transparent" />
            </MultiDataTrigger>
            <MultiDataTrigger>
                <MultiDataTrigger.Conditions>
                    <Condition Binding="{Binding Type}" Value="NG" />
                    <!--<Condition Binding="{Binding IsThereABattery}" Value="True" />-->
                </MultiDataTrigger.Conditions>
                <Setter Property="BorderBrush" Value="{DynamicResource PrimaryHueLightBrush}" />
                <Setter Property="Background" Value="Red" />
            </MultiDataTrigger>

            <DataTrigger Binding="{Binding Type}" Value="Detect">
                <Setter Property="BorderBrush" Value="{DynamicResource PrimaryHueLightBrush}" />
                <Setter Property="Background" Value="Yellow" />
            </DataTrigger>

            <MultiDataTrigger>
                <MultiDataTrigger.Conditions>
                    <Condition Binding="{Binding Type}" Value="Detect" />
                    <Condition Binding="{Binding WhetherThereAreFakeBatteries}" Value="true" />
                </MultiDataTrigger.Conditions>
                <Setter Property="BorderBrush" Value="Blue" />
                <Setter Property="Background" Value="Yellow" />
            </MultiDataTrigger>

            <MultiDataTrigger>
                <MultiDataTrigger.Conditions>
                    <Condition Binding="{Binding Type}" Value="Detect" />
                    <Condition Binding="{Binding WhetherThereAreFakePosBatteries}" Value="true" />
                </MultiDataTrigger.Conditions>
                <Setter Property="BorderBrush" Value="Blue" />
                <Setter Property="Background" Value="Yellow" />
            </MultiDataTrigger>

            <DataTrigger Binding="{Binding Type}" Value="WaitRes">
                <Setter Property="BorderBrush" Value="{DynamicResource PrimaryHueLightBrush}" />
                <Setter Property="Background" Value="Yellow" />
            </DataTrigger>
            <MultiDataTrigger>
                <MultiDataTrigger.Conditions>
                    <Condition Binding="{Binding Type}" Value="WaitRes" />
                    <Condition Binding="{Binding WhetherThereAreFakePosBatteries}" Value="true" />
                </MultiDataTrigger.Conditions>
                <Setter Property="BorderBrush" Value="#996515" />
                <Setter Property="Background" Value="Yellow" />
            </MultiDataTrigger>

            <DataTrigger Binding="{Binding Type}" Value="WaitOffload">
                <Setter Property="BorderBrush" Value="{DynamicResource PrimaryHueLightBrush}" />
                <Setter Property="Background" Value="Yellow" />
            </DataTrigger>

            <DataTrigger Binding="{Binding Type}" Value="WaitRebakeBat">
                <Setter Property="BorderBrush" Value="{DynamicResource PrimaryHueLightBrush}" />
                <Setter Property="Background" Value="Magenta" />

            </DataTrigger>

            <MultiDataTrigger>
                <MultiDataTrigger.Conditions>
                    <Condition Binding="{Binding Type}" Value="WaitRebakeBat" />
                    <Condition Binding="{Binding WhetherThereAreFakePosBatteries}" Value="true" />
                </MultiDataTrigger.Conditions>
                <Setter Property="BorderBrush" Value="#996515" />
                <Setter Property="Background" Value="Magenta" />
            </MultiDataTrigger>

            <MultiDataTrigger>
                <MultiDataTrigger.Conditions>
                    <Condition Binding="{Binding Type}" Value="WaitRebakeBat" />
                    <Condition Binding="{Binding WhetherThereAreRBFakeBatteries}" Value="true" />
                </MultiDataTrigger.Conditions>
                <Setter Property="BorderBrush" Value="#FFD700" />
                <Setter Property="Background" Value="Magenta" />
            </MultiDataTrigger>

            <DataTrigger Binding="{Binding Type}" Value="WaitRebakingToOven">
                <Setter Property="BorderBrush" Value="{DynamicResource PrimaryHueLightBrush}" />
                <Setter Property="Background" Value="Magenta" />
            </DataTrigger>

            <MultiDataTrigger>
                <MultiDataTrigger.Conditions>
                    <Condition Binding="{Binding Type}" Value="WaitRebakingToOven" />
                    <Condition Binding="{Binding WhetherThereAreRBFakeBatteries}" Value="true" />
                </MultiDataTrigger.Conditions>
                <Setter Property="BorderBrush" Value="#8A2BE2" />
                <Setter Property="Background" Value="Magenta" />
            </MultiDataTrigger>

            <DataTrigger Binding="{Binding PltPos}">
                <DataTrigger.Value>
                    <sys:Boolean>False</sys:Boolean>
                </DataTrigger.Value>
                <Setter Property="BorderBrush" Value="{DynamicResource PrimaryHueLightBrush}" />
                <Setter Property="Background" Value="Transparent" />
            </DataTrigger>




        </Style.Triggers>
    </Style>

    <Style x:Key="OvenState" TargetType="Border">
        <Setter Property="Margin" Value="4,0,4,4" />
        <Setter Property="BorderBrush" Value="Aqua" />
        <Setter Property="BorderThickness" Value="0.5" />
        <Style.Triggers>
            <DataTrigger Binding="{Binding State}" Value="Invalid">
                <Setter Property="Background" Value="White" />
            </DataTrigger>
            <DataTrigger Binding="{Binding State}" Value="Standby">
                <Setter Property="Background" Value="White" />
            </DataTrigger>
            <DataTrigger Binding="{Binding State}" Value="Work">
                <Setter Property="Background" Value="Yellow" />
            </DataTrigger>
            <DataTrigger Binding="{Binding State}" Value="Detect">
                <Setter Property="Background" Value="#d9e3ff" />
            </DataTrigger>
            <DataTrigger Binding="{Binding State}" Value="WaitRes">
                <Setter Property="Background" Value="#83c2c7" />
            </DataTrigger>
            <DataTrigger Binding="{Binding State}" Value="Rebaking">
                <Setter Property="Background" Value="Magenta" />
            </DataTrigger>
        </Style.Triggers>
    </Style>

    <Style x:Key="BatteryStyle" TargetType="Border">
        <Style.Triggers>
            <DataTrigger Binding="{Binding Type}" Value="Invalid">
                <Setter Property="BorderBrush" Value="Gray" />

            </DataTrigger>
            <DataTrigger Binding="{Binding Type}" Value="TypeEnd">
                <Setter Property="BorderBrush" Value="Gray" />
                <Setter Property="Background" Value="Gray" />

            </DataTrigger>

            <DataTrigger Binding="{Binding Type}" Value="OK">
                <Setter Property="BorderBrush" Value="Green" />
                <Setter Property="Background" Value="Green" />
            </DataTrigger>

            <DataTrigger Binding="{Binding Type}" Value="NG">
                <Setter Property="BorderBrush" Value="Red" />
                <Setter Property="Background" Value="Red" />
            </DataTrigger>

            <DataTrigger Binding="{Binding Type}" Value="Fake">
                <Setter Property="BorderBrush" Value="Blue" />
                <Setter Property="Background" Value="Blue" />
            </DataTrigger>

            <DataTrigger Binding="{Binding Type}" Value="FakePos">
                <Setter Property="BorderBrush" Value="LightBlue" />
                <Setter Property="Background" Value="LightBlue" />
            </DataTrigger>

            <DataTrigger Binding="{Binding Type}" Value="RBFake">
                <Setter Property="BorderBrush" Value="#FFD700" />
                <Setter Property="Background" Value="#FFD700" />
            </DataTrigger>

            <DataTrigger Binding="{Binding Type}" Value="BKFill">
                <Setter Property="BorderBrush" Value="LightGreen" />
                <Setter Property="Background" Value="LightGreen" />
            </DataTrigger>


        </Style.Triggers>
    </Style>

    <Style x:Key="OvenConnect" TargetType="TextBox">
        <Style.Triggers>
            <DataTrigger Binding="{Binding Type}" Value="Invalid">
                <Setter Property="BorderBrush" Value="{DynamicResource PrimaryHueLightBrush}" />

            </DataTrigger>
        </Style.Triggers>
    </Style>

    <Style x:Key="MainTextBlock" TargetType="TextBlock">
        <Setter Property="Foreground" Value="{DynamicResource PrimaryBackgroundColor}" />
        <Style.Triggers>
            <DataTrigger Binding="{Binding Source={x:Static machine:MachineCtrl.MachineCtrlInstance}, Path=RunsCtrl.McState}" Value="MCIdle">
                <Setter Property="Text" Value="{DynamicResource 闲置}" />
                <Setter Property="Tag" Value="#555555" />
            </DataTrigger>
            <DataTrigger Binding="{Binding Source={x:Static machine:MachineCtrl.MachineCtrlInstance}, Path=RunsCtrl.McState}" Value="MCInitializing">
                <Setter Property="Text" Value="{DynamicResource 初始化中}" />
                <Setter Property="Tag" Value="#00FF00" />
            </DataTrigger>
            <DataTrigger Binding="{Binding Source={x:Static machine:MachineCtrl.MachineCtrlInstance}, Path=RunsCtrl.McState}" Value="MCInitComplete">
                <Setter Property="Text" Value="{DynamicResource 初始化完成}" />
                <Setter Property="Tag" Value="Green" />

            </DataTrigger>
            <DataTrigger Binding="{Binding Source={x:Static machine:MachineCtrl.MachineCtrlInstance}, Path=RunsCtrl.McState}" Value="MCRunning">
                <Setter Property="Text" Value="{DynamicResource 运行中}" />
                <Setter Property="Tag" Value="#43CF7C" />

            </DataTrigger>
            <DataTrigger Binding="{Binding Source={x:Static machine:MachineCtrl.MachineCtrlInstance}, Path=RunsCtrl.McState}" Value="MCStopInit">
                <Setter Property="Text" Value="{DynamicResource 初始化停止}" />
                <Setter Property="Tag" Value="Yellow" />

            </DataTrigger>
            <DataTrigger Binding="{Binding Source={x:Static machine:MachineCtrl.MachineCtrlInstance}, Path=RunsCtrl.McState}" Value="MCStopRun">
                <Setter Property="Text" Value="{DynamicResource 运行停止}" />
                <Setter Property="Tag" Value="#FCB31C" />

            </DataTrigger>
            <DataTrigger Binding="{Binding Source={x:Static machine:MachineCtrl.MachineCtrlInstance}, Path=RunsCtrl.McState}" Value="MCInitErr">
                <Setter Property="Text" Value="{DynamicResource 初始化错误}" />
                <Setter Property="Tag" Value="Red" />

            </DataTrigger>
            <DataTrigger Binding="{Binding Source={x:Static machine:MachineCtrl.MachineCtrlInstance}, Path=RunsCtrl.McState}" Value="MCRunErr">
                <Setter Property="Text" Value="{DynamicResource 运行错误}" />
                <Setter Property="Tag" Value="Red" />

            </DataTrigger>
        </Style.Triggers>
    </Style>



    <Style x:Key="OvenEnableStyle" TargetType="Border">
        <Style.Triggers>
            <DataTrigger Binding="{Binding OvenEnable}" Value="true">
                <Setter Property="Margin" Value="6,11,6,6" />
                <Setter Property="Opacity" Value="0" />
            </DataTrigger>
            <DataTrigger Binding="{Binding OvenEnable}" Value="False">
                <Setter Property="BorderBrush" Value="Black" />
                <Setter Property="Margin" Value="6,11,6,6" />
                <Setter Property="Opacity" Value="0.3" />
            </DataTrigger>
        </Style.Triggers>
    </Style>

    <Style x:Key="OvenDoorStyle" TargetType="Border">
        <Style.Triggers>
            <DataTrigger Binding="{Binding RealTimeData.DoorState}" Value="Invalid">
                <Setter Property="Margin" Value="4.5,0,5,4.5" />
            </DataTrigger>
            <DataTrigger Binding="{Binding RealTimeData.DoorState}" Value="Action">
                <Setter Property="Margin" Value="4.5,0,35,4.5" />
            </DataTrigger>
            <DataTrigger Binding="{Binding RealTimeData.DoorState}" Value="Open">
                <Setter Property="Margin" Value="4.5,0,65,4.5" />
            </DataTrigger>
            <DataTrigger Binding="{Binding RealTimeData.DoorState}" Value="Close">
                <Setter Property="Margin" Value="4.5,0,5,4.5" />
            </DataTrigger>
        </Style.Triggers>
    </Style>


    <Style x:Key="OvenEnablelockStyle" TargetType="Path">
        <Style.Triggers>
            <DataTrigger Binding="{Binding OvenEnable}" Value="true">
                <Setter Property="Opacity" Value="0" />
            </DataTrigger>
            <DataTrigger Binding="{Binding OvenEnable}" Value="false">
                <Setter Property="Opacity" Value="1" />
            </DataTrigger>
        </Style.Triggers>
    </Style>

    <Style x:Key="OvenPressurizeStylePath" TargetType="Path">
        <Style.Triggers>
            <DataTrigger Binding="{Binding Pressure}" Value="true">
                <Setter Property="Opacity" Value="0.4" />
            </DataTrigger>
            <DataTrigger Binding="{Binding Pressure}" Value="false">
                <Setter Property="Opacity" Value="0" />
            </DataTrigger>
        </Style.Triggers>
    </Style>

    <Style x:Key="OvenPressurizeStyle" TargetType="Border">
        <Style.Triggers>
            <DataTrigger Binding="{Binding Pressure}" Value="true">
                <Setter Property="Margin" Value="6,11,6,6" />
                <Setter Property="Opacity" Value="0.7" />
            </DataTrigger>
            <DataTrigger Binding="{Binding Pressure}" Value="False">
                <Setter Property="Opacity" Value="0" />
            </DataTrigger>
        </Style.Triggers>
    </Style>

