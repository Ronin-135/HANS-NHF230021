    <Style x:Key="OvenPalletStyle" TargetType="Border">
        <Style.Triggers>
            <DataTrigger Binding="{Binding Type}" Value="Invalid">
                <Setter Property="Background" Value="Transparent" />
            </DataTrigger>

            <!--<DataTrigger Binding="{Binding Type}" Value="OK">
                <Setter Property="Background" Value="Black" />
                <Setter Property="Opacity" Value="0.2" />
            </DataTrigger>-->


            <DataTrigger Binding="{Binding Type}" Value="NG">
                <Setter Property="Background" Value="Red" />
            </DataTrigger>

            <MultiDataTrigger>
                <MultiDataTrigger.Conditions>
                    <Condition Binding="{Binding Type}" Value="OK" />
                    <Condition Binding="{Binding WhetherThereAreFakeBatteries}" Value="True" />
                    <!--<Condition Binding="{Binding Stage}" Value="Onload" />-->
                </MultiDataTrigger.Conditions>
                <Setter Property="BorderBrush" Value="Blue" />
                <Setter Property="BorderThickness" Value="2" />
                <Setter Property="Background" Value="Green" />
            </MultiDataTrigger>


            <MultiDataTrigger>
                <MultiDataTrigger.Conditions>
                    <Condition Binding="{Binding Type}" Value="OK" />
                    <Condition Binding="{Binding WhetherThereAreFakeBatteries}" Value="False" />
                </MultiDataTrigger.Conditions>
                <Setter Property="BorderBrush" Value="Black" />
                <Setter Property="BorderThickness" Value="0.5" />
                <Setter Property="Background" Value="Green" />
            </MultiDataTrigger>

            <MultiDataTrigger>
                <MultiDataTrigger.Conditions>
                    <Condition Binding="{Binding Type}" Value="OK" />
                    <Condition Binding="{Binding WhetherThereAreIsEmpty}" Value="true" />
                </MultiDataTrigger.Conditions>
                <Setter Property="BorderBrush" Value="Black" />
                <Setter Property="BorderThickness" Value="0.5" />
                <Setter Property="Background" Value="LightGray" />
            </MultiDataTrigger>

            <MultiDataTrigger>
                <MultiDataTrigger.Conditions>
                    <Condition Binding="{Binding Type}" Value="Detect" />
                    <Condition Binding="{Binding WhetherThereAreFakeBatteries}" Value="true" />
                </MultiDataTrigger.Conditions>
                <Setter Property="BorderBrush" Value="Blue" />
                <Setter Property="BorderThickness" Value="2" />
                <Setter Property="Background" Value="Aqua" />
            </MultiDataTrigger>


            <MultiDataTrigger>
                <MultiDataTrigger.Conditions>
                    <Condition Binding="{Binding Type}" Value="Detect" />
                    <Condition Binding="{Binding WhetherThereAreFakeBatteries}" Value="False" />
                </MultiDataTrigger.Conditions>
                <Setter Property="BorderBrush" Value="Black" />
                <Setter Property="BorderThickness" Value="0.5" />
                <Setter Property="Background" Value="Aqua" />
            </MultiDataTrigger>

            <MultiDataTrigger>
                <MultiDataTrigger.Conditions>
                    <Condition Binding="{Binding Type}" Value="WaitRes" />
                    <Condition Binding="{Binding WhetherThereAreFakeBatteries}" Value="true" />
                </MultiDataTrigger.Conditions>
                <Setter Property="BorderBrush" Value="Blue" />
                <Setter Property="BorderThickness" Value="2" />
                <Setter Property="Background" Value="Yellow" />
            </MultiDataTrigger>


            <MultiDataTrigger>
                <MultiDataTrigger.Conditions>
                    <Condition Binding="{Binding Type}" Value="WaitRes" />
                    <Condition Binding="{Binding WhetherThereAreFakeBatteries}" Value="False" />
                </MultiDataTrigger.Conditions>
                <Setter Property="BorderBrush" Value="Black" />
                <Setter Property="BorderThickness" Value="0.5" />
                <Setter Property="Background" Value="Yellow" />
            </MultiDataTrigger>

            <MultiDataTrigger>
                <MultiDataTrigger.Conditions>
                    <Condition Binding="{Binding Type}" Value="WaitOffload" />
                    <Condition Binding="{Binding WhetherThereAreFakeBatteries}" Value="true" />
                </MultiDataTrigger.Conditions>
                <Setter Property="BorderBrush" Value="Blue" />
                <Setter Property="BorderThickness" Value="1" />
                <Setter Property="Background" Value="Orange" />
            </MultiDataTrigger>


            <MultiDataTrigger>
                <MultiDataTrigger.Conditions>
                    <Condition Binding="{Binding Type}" Value="WaitOffload" />
                    <Condition Binding="{Binding WhetherThereAreFakeBatteries}" Value="False" />
                </MultiDataTrigger.Conditions>
                <Setter Property="BorderBrush" Value="Black" />
                <Setter Property="BorderThickness" Value="0.5" />
                <Setter Property="Background" Value="Orange" />
            </MultiDataTrigger>
            <MultiDataTrigger>
                <MultiDataTrigger.Conditions>
                    <Condition Binding="{Binding Type}" Value="WaitCooling" />
                    <Condition Binding="{Binding WhetherThereAreFakeBatteries}" Value="true" />
                </MultiDataTrigger.Conditions>
                <Setter Property="BorderBrush" Value="Blue" />
                <Setter Property="BorderThickness" Value="1" />
                <Setter Property="Background" Value="Yellow" />
                <Setter Property="Opacity" Value="0.9" />

            </MultiDataTrigger>


            <MultiDataTrigger>
                <MultiDataTrigger.Conditions>
                    <Condition Binding="{Binding Type}" Value="WaitCooling" />
                    <Condition Binding="{Binding WhetherThereAreFakeBatteries}" Value="False" />
                </MultiDataTrigger.Conditions>
                <Setter Property="BorderBrush" Value="Black" />
                <Setter Property="BorderThickness" Value="0.5" />
                <Setter Property="Opacity" Value="0.9" />
                <Setter Property="Background" Value="Yellow" />
            </MultiDataTrigger>

            <MultiDataTrigger>
                <MultiDataTrigger.Conditions>
                    <Condition Binding="{Binding Type}" Value="WaitRebakeBat" />
                    <Condition Binding="{Binding WhetherThereAreFakeBatteries}" Value="true" />
                </MultiDataTrigger.Conditions>
                <Setter Property="BorderBrush" Value="Blue" />
                <Setter Property="BorderThickness" Value="2" />
                <Setter Property="Background" Value="Magenta" />
            </MultiDataTrigger>

            <MultiDataTrigger>
                <MultiDataTrigger.Conditions>
                    <Condition Binding="{Binding Type}" Value="WaitRebakeBat" />
                    <Condition Binding="{Binding WhetherThereAreFakeBatteries}" Value="False" />
                </MultiDataTrigger.Conditions>
                <Setter Property="BorderBrush" Value="Black" />
                <Setter Property="BorderThickness" Value="0.5" />
                <Setter Property="Background" Value="Magenta" />
            </MultiDataTrigger>

        </Style.Triggers>
    </Style>

    <!--  DataGrid 行样式  -->
    <Style x:Key="MyDataGridRowSytle" TargetType="DataGridRow">
        <Setter Property="Height" Value="22" />
        <Setter Property="Foreground" Value="Black" />
        <Setter Property="VerticalAlignment" Value="Center" />
        <Setter Property="HorizontalAlignment" Value="Left" />
        <Setter Property="BorderThickness" Value="0" />
        <Style.Triggers>
            <!--  隔行换色 放到前面  -->
            <Trigger Property="AlternationIndex" Value="0">
                <Setter Property="Background" Value="LightBlue" />
            </Trigger>
            <Trigger Property="AlternationIndex" Value="1">
                <Setter Property="Background" Value="LightYellow" />
            </Trigger>
            <!--  选中时背景颜色  -->
            <Trigger Property="IsSelected" Value="True">
                <Setter Property="Background" Value="Blue" />
            </Trigger>
            <MultiTrigger>
                <!--  失去焦点时背景颜色  -->
                <MultiTrigger.Conditions>
                    <Condition Property="IsSelected" Value="true" />
                    <Condition Property="Selector.IsSelectionActive" Value="false" />
                </MultiTrigger.Conditions>
                <Setter Property="Background" Value="Pink" />
            </MultiTrigger>
        </Style.Triggers>
    </Style>
    <!--  DataGrid 单元格样式  -->
    <Style
        x:Key="MyDataGridCellSytle"
        BasedOn="{StaticResource MaterialDesignDataGridCell}"
        TargetType="DataGridCell">
        <Setter Property="VerticalContentAlignment" Value="Center" />
        <Setter Property="VerticalAlignment" Value="Center" />
        <Setter Property="VerticalContentAlignment" Value="Center" />
        <Setter Property="BorderThickness" Value="0" />
        <Style.Triggers>
            <!--  选中时背景颜色  -->
            <Trigger Property="IsSelected" Value="True">
                <Setter Property="Background" Value="{DynamicResource BorderColor}" />
            </Trigger>

            <MultiTrigger>
                <!--  失去焦点时背景颜色  -->
                <MultiTrigger.Conditions>
                    <Condition Property="IsSelected" Value="true" />
                    <Condition Property="Selector.IsSelectionActive" Value="false" />
                </MultiTrigger.Conditions>
                <Setter Property="Background" Value="{DynamicResource BorderColor}" />
            </MultiTrigger>
        </Style.Triggers>
    </Style>
    <!--  DataGrid 单元格标题头样式  -->
    <Style
        x:Key="DataGridColHeader"
        BasedOn="{StaticResource MaterialDesignDataGridColumnHeader}"
        TargetType="DataGridColumnHeader">
        <Setter Property="FontSize" Value="12" />
        <Setter Property="Height" Value="40" />
        <Setter Property="HorizontalContentAlignment" Value="Center" />
    </Style>

    <!--  列的文本居中  -->
    <Style x:Key="DataGridTextColumnCenterSytle" TargetType="{x:Type TextBlock}">
        <Setter Property="HorizontalAlignment" Value="Center" />
        <Setter Property="VerticalAlignment" Value="Center" />
    </Style>

    <Style
        x:Key="IOButton"
        BasedOn="{StaticResource MaterialDesignOutlinedLightButton}"
        TargetType="Button">
        <Style.Triggers>
            <Trigger Property="IsEnabled" Value="False" />
        </Style.Triggers>
    </Style>

    <Style x:Key="CavityStyle" TargetType="Border">
        <Style.Triggers>
            <DataTrigger Binding="{Binding State}" Value="Standby">
                <Setter Property="Background" Value="Transparent" />
            </DataTrigger>
            <MultiDataTrigger>
                <MultiDataTrigger.Conditions>
                    <Condition Binding="{Binding State}" Value="Work" />
                </MultiDataTrigger.Conditions>
                <Setter Property="Background" Value="Yellow" />
            </MultiDataTrigger>
            <MultiDataTrigger>
                <MultiDataTrigger.Conditions>
                    <Condition Binding="{Binding State}" Value="Detect" />
                </MultiDataTrigger.Conditions>
                <Setter Property="Background" Value="Cyan" />
            </MultiDataTrigger>
            <MultiDataTrigger>
                <MultiDataTrigger.Conditions>
                    <Condition Binding="{Binding State}" Value="WaitRes" />
                </MultiDataTrigger.Conditions>
                <Setter Property="Background" Value="#ff008b8b" />
                <!--  BackgroundColor "{Name=ff008b8b, ARGB=(255, 0, 139, 139)}"  -->
            </MultiDataTrigger>
            <MultiDataTrigger>
                <MultiDataTrigger.Conditions>
                    <Condition Binding="{Binding State}" Value="Rebaking" />
                </MultiDataTrigger.Conditions>
                <Setter Property="Background" Value="#ffff00ff" />
                <!--  BackgroundColor "{Name=ffff00ff, ARGB=(255, 255, 0, 255)}"  -->
            </MultiDataTrigger>
            <MultiDataTrigger>
                <MultiDataTrigger.Conditions>
                    <Condition Binding="{Binding State}" Value="Maintenance" />
                </MultiDataTrigger.Conditions>
                <Setter Property="Background" Value="#ffff0000" />
                <!--  BackgroundColor "{Name=ffff0000, ARGB=(255, 255, 0, 0)}"  -->
            </MultiDataTrigger>
            <MultiDataTrigger>
                <MultiDataTrigger.Conditions>
                    <Condition Binding="{Binding State}" Value="Transfer" />
                </MultiDataTrigger.Conditions>
